package {		import flash.display.MovieClip;	import flash.events.*;		public class notificationInvincible extends MovieClip {						public function notificationInvincible() {			this.addEventListener(Event.ENTER_FRAME, MoveNotiInvincible);		}		private function MoveNotiInvincible(e:Event)		{			this.x -= 5;						if (x < -100)			{				this.removeEventListener(Event.ENTER_FRAME, MoveNotiInvincible);				parent.removeChild(this);			}		}	}	}