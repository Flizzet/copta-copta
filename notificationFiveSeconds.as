package {		import flash.display.MovieClip;	import flash.events.*;	public class notificationFiveSeconds extends MovieClip{		public function notificationFiveSeconds()		{			this.addEventListener(Event.ENTER_FRAME, MoveNotiFiveSeconds);		}		private function MoveNotiFiveSeconds(e:Event)		{			this.x -= 5;						if (x < -100)			{				this.removeEventListener(Event.ENTER_FRAME, MoveNotiFiveSeconds);				parent.removeChild(this);			}		}	}}